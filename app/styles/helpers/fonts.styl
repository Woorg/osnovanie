@font-face
	font-family "Merriweather"
	src local("Merriweather Bold"),
		local("Merriweather-Bold"),
		url("../fonts/hinted-subset-Merriweather-Bold.woff2") format("woff2"),
		url("../fonts/hinted-subset-Merriweather-Bold.woff") format("woff")
	font-weight 700
	font-style normal

@font-face
	font-family "Merriweather"
	src local("Merriweather Light"),
		local("Merriweather-Light"),
		url("../fonts/hinted-subset-Merriweather-Light.woff2") format("woff2"),
		url("../fonts/hinted-subset-Merriweather-Light.woff") format("woff")
	font-weight 300
