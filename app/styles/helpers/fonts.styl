@font-face
	font-family "Museo Cyrl 700"
	src local("Museo Cyrl 700"), 
		local("MuseoCyrl-700"),
		url("../fonts/hinted-subset-MuseoCyrl-700.woff2") format("woff2"),
		url("../fonts/hinted-subset-MuseoCyrl-700.woff") format("woff")
	font-weight 700
	font-style normal

