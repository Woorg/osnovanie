	font-family "Merriweather"
	src local("Merriweather Regular"),
		local("Merriweather-Regular"),
		url("../fonts/hinted-subset-Merriweather-Regular.woff2") format("woff2"),
		url("../fonts/hinted-subset-Merriweather-Regular.woff") format("woff")
	font-weight 400
	font-style normal

// museo
$mB = 'Museo Cyrl 700' , Arial,'Helvetica Neue',Helvetica,sans-serif
$mL = 'Museo Cyrl 300' , Arial,'Helvetica Neue',Helvetica,sans-serif
$mM = 'Museo Cyrl 500' , Arial,'Helvetica Neue',Helvetica,sans-serif

// merriweather
$me = 'Merriweather' , Arial,'Helvetica Neue',Helvetica,sans-serif

