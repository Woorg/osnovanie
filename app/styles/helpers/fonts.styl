	font-family "Museo Cyrl 300"
	src local("Museo Cyrl 300"),
		local("MuseoCyrl-300"),
		url("../fonts/hinted-subset-MuseoCyrl-300.woff2") format("woff2"),
		url("../fonts/hinted-subset-MuseoCyrl-300.woff") format("woff")
	font-weight 300
	font-style normal

@font-face
	font-family "Museo Cyrl 300"
	src local("Museo Cyrl 300 Italic"),
		local("MuseoCyrl-300Italic"),
		url("../fonts/hinted-subset-MuseoCyrl-300Italic.woff2") format("woff2"),
		url("../fonts/hinted-subset-MuseoCyrl-300Italic.woff") format("woff")
	font-weight 300
	font-style italic

